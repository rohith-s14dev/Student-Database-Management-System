outfile: main.o save.o add_end.o exit.o print.o stud_mod.o rev_data.o stud_sort.o count.o menu.o delete.o deleteall.o
	cc main.c save.c add_end.c exit.c print.c stud_mod.c rev_data.c stud_sort.c count.c menu.c delete.c deleteall.c -o outfile

main.o: main.c
	cc -c main.c
save.o: save.c
	cc -c save.c
add_end.o: add_end.c
	cc -c add_end.c
exit.o: exit.c
	cc -c exit.c
print.o: print.c
	cc -c print.c
stud_mod.o: stud_mod.c
	cc -c stud_mod.c
rev_data.o: rev_data.c
	cc -c rev_data.c
stud_sort.o: stud_sort.c
	cc -c stud_sort.c
count.o:count.c
	cc -c count.c
menu.o:menu.c
	cc -c menu.c
delete.o:delete.c
	cc -c delete.c
deleteall.o:deleteall.c
	cc -c deleteall.c


